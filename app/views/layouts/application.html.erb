<!DOCTYPE html>
<html>
<head>
  <title>Asubal</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
        <div class="nav-wrapper black">
          <a href="#" class="brand-logo center "><%= link_to 'Assignment Submission Portal', root_path, class: "brand-logo center" %> </a>
          <ul id="nav-mobile" class="right hide-on-med-and-down">
            <li><%= link_to  "Home", root_path %></li>
            <% if teacher_signed_in? %> <li><%= link_to  "Sign Out Teacher", destroy_teacher_session_path, :method => :delete %></li> <%end%>
            <% if student_signed_in? %> <li><%= link_to  "Sign Out Student", destroy_student_session_path, :method => :delete, class: "right hide-on-med-and-down" %></li> <%end%>
            <%#  <li><%= link_to  "Generate Qr", new_qrcode_path %>   <%#  </li>
            <% if student_signed_in? %>  <%# <li><%= link_to  "Sign Out", destroy_user_session_path, :method => :delete %>  <%#   </li> <%end%>
          </ul>
        </div>
      </nav>
<div class="container">
<%= yield %>
</div>
</body>
</html>
